CREATE TABLE events (
    id INTEGER NOT NULL,
    timestamp INTEGER NOT NULL,
    type INTEGER NOT NULL,
    uri TEXT,
    metadata TEXT, // json

    isDiscrete BOOL,
    isStart BOOL,
    otherEndpoint INTEGER NOT NULL,
    duration INTEGER NOT NULL,
);

CREATE TABLE event_types (
    id INTEGER NOT NULL,
    parent INTEGER NOT NULL,
    label TEXT,
    tags TEXT,
);

CREATE TABLE IF NOT EXISTS user (
    name TEXT NOT NULL,
    password TEXT NOT NULL,
);
